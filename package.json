{
  "name": "tre-stylesheets",
  "version": "1.0.0",
  "description": "CSS stlyesheets for tre",
  "main": "main.js",
  "browser": "index.js",
  "scripts": {
    "test": "tape test/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/regular/tre-styles.git"
  },
  "keywords": [
    "tre",
    "ssb"
  ],
  "author": "Jan BÃ¶lsche <jan@lagomorph.de> (https://regular.codes/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/regular/tre-styles/issues"
  },
  "homepage": "https://github.com/regular/tre-styles#readme",
  "tre-init": {
    "plugins": {
      "tre-client": true,
      "ssb-revisions": true
    },
    "prototypes": {
      ".": true
    },
    "importers": {
      ".": true
    },
    "files": {
      "example.css": {
        "path": "example.css",
        "content": {
          "branch": "%stylesheets",
          "root": "%root"
        }
      }
    }
  },
  "devDependencies": {
    "ssb-revisions": "^1.2.1",
    "tape": "^4.9.2",
    "tre-client": "^2.1.1",
    "tre-file-importer": "^2.1.0",
    "tre-finder": "^2.3.0",
    "tre-prototypes": "^1.6.0",
    "tre-split-pane": "^1.0.1"
  },
  "dependencies": {
    "bl": "^2.1.2",
    "brace": "^0.11.1",
    "module-styles": "^1.0.0",
    "mutant": "^3.22.1",
    "pull-stream": "^3.6.9",
    "tre-string": "^1.0.1"
  }
}
